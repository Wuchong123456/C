# include <stdio.h>
# include <stdlib.h>
int main(void)
{
	int *p = malloc(sizeof*p);
	*p = 10;
	printf("p = %p\n", p);
	printf("*p = %d\n", *p);
	free(p);
	printf("p = %p\n", p);
	printf("*p = %d\n", *p);
	return 0;
}

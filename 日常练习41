struct S
{
	int n;
	float score;
	char arr[10];
};
int main()
{
	struct S s = { 100, 3.14f, "bit" };
	FILE* pf = fopen("test.txt", "w");
	if (pf = NULL)
	{
		/*printf("%s\n", strerror(errno));*/
		return 0;
	}
	printf("%d %f %s", s.n, s.score, s.arr);
	//fprintf(pf, "%d %f %s", s.n, s.score, s.arr);
	fclose(pf);
	pf = NULL;
	return 0;
}
